#!/bin/bash

# This bash script starts building a collection of base docker images for 
# use by Laminar. 

DIST=bionic
echo docker image rm laminar_root_$DIST
docker image rm laminar_root_$DIST
echo docker image rm laminar_dev_$DIST
docker image rm laminar_dev_$DIST
echo laminarc queue buildDockerUbuntuBase DIST=$DIST
laminarc queue buildDockerUbuntuBase DIST=$DIST

DIST=eoan
echo docker image rm laminar_root_$DIST
docker image rm laminar_root_$DIST
echo docker image rm laminar_dev_$DIST
docker image rm laminar_dev_$DIST
echo laminarc queue buildDockerUbuntuBase DIST=$DIST
laminarc queue buildDockerUbuntuBase DIST=$DIST

DIST=focal
secho docker image rm laminar_root_$DIST
docker image rm laminar_root_$DIST
echo docker image rm laminar_dev_$DIST
docker image rm laminar_dev_$DIST
echo laminarc queue buildDockerUbuntuBase DIST=$DIST
laminarc queue buildDockerUbuntuBase DIST=$DIST

DIST=3.12
echo docker image rm laminar_root_$DIST
docker image rm laminar_root_$DIST
echo docker image rm laminar_dev_$DIST
docker image rm laminar_dev_$DIST
echo laminarc queue buildDockerAlpineBase DIST=$DIST
laminarc queue buildDockerAlpineBase DIST=$DIST
